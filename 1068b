#include <stdio.h>
#include <stdlib.h>
#include <string.h>

typedef struct No {
    char simbolo;
    struct No *prox;
} No;

void push(No **topo, char c) {
    No *novo = (No*) malloc(sizeof(No));
    novo->simbolo = c;
    novo->prox = *topo;
    *topo = novo;
}

int pop(No **topo) {
    if (*topo == NULL)
        return 0; // erro: pilha vazia
    No *temp = *topo;
    *topo = (*topo)->prox;
    free(temp);
    return 1;
}

void liberar(No **topo) {
    while (*topo != NULL) {
        No *temp = *topo;
        *topo = (*topo)->prox;
        free(temp);
    }
}

int main() {
    char expressao[1001];

    while (fgets(expressao, sizeof(expressao), stdin)) {
        No *pilha = NULL;
        int incorreto = 0;

        for (int i = 0; expressao[i] != '\0'; i++) {
            if (expressao[i] == '(') {
                push(&pilha, '(');
            } else if (expressao[i] == ')') {
                if (!pop(&pilha)) { // tenta desempilhar
                    incorreto = 1;
                    break;
                }
            }
        }

        if (pilha == NULL && !incorreto)
            printf("correct\n");
        else
            printf("incorrect\n");

        liberar(&pilha); // limpa mem√≥ria
    }

    return 0;
}
