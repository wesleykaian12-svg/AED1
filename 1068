#include <stdio.h>
#include <string.h>

int main() {
    char expressao[1001];
    
    
    while (fgets(expressao, sizeof(expressao), stdin)) {
        int contador = 0;
        int incorreto = 0;

        for (int i = 0; expressao[i] != '\0'; i++) {
            if (expressao[i] == '(') {
                contador++;
            } else if (expressao[i] == ')') {
                contador--;
                if (contador < 0) { // mais ')' do que '('
                    incorreto = 1;
                    break;
                }
            }
        }

       
        if (contador != 0 || incorreto)
            printf("incorrect\n");
        else
            printf("correct\n");
    }

    return 0;
}
